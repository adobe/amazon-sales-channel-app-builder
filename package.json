    "@formatjs/cli": "^5.1.4",
    "@openwhisk/wskdebug": "^1.4.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.192",
    "@types/node": "^18.15.10",
    "@types/node-fetch": "^2.6.4",
    "@types/react": "^18.0.30",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.38.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-formatter-pretty": "^5.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "husky": "^8.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.4.1",
    "prettier": "^2.8.7",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build": "npm run clean && npm run compile && aio app build",
    "check": "tsc --noEmit",
    "clean": "rm -rf actions",
    "compile": "npm run clean && tsc",
    "deploy": "npm run build && aio app deploy",
    "e2e": "npm run test:e2e",
    "extract": "formatjs extract 'web-src/src/components/**/*.{ts,tsx,jsx,js}' --ignore='**/*.d.ts' --out-file ./extracted/en.json",
    "format": "prettier --check \"./**/*.{ts,tsx,js,css,json,md}\"",
    "format:fix": "npm run lint -- --fix; prettier --write \"./**/*.{ts,tsx,js,jsx,css,json,md}\"",
    "prepare": "husky install",
    "test:e2e": "jest --collectCoverage=false --passWithNoTests ./e2e",
    "lint:fix": "npm run lint -- --fix",
    "lint": "eslint actions-src test web-src e2e --ext .js,.jsx,.ts,.tsx",
    "test": "npm run test:actions && npm run test:web && npm run test:e2e",
    "test:actions": "jest --passWithNoTests ./test/actions",
    "test:actions:watch": "jest --passWithNoTests ./test/actions --watch",
    "test:web": "jest --passWithNoTests --color --config=jest.web.config.js ./test/web-src",
    "test:web:watch": "jest --passWithNoTests --color --watch --config=jest.web.config.js ./test/web-src"
  },
  "engines": {
    "node": "^16.13 || >=18"
  },
  "plugins": [
    "react-hooks"
  ],
  "rules": {
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn"
  }
}
